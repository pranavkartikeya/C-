#include<bits/stdc++.h>

using namespace std;
int main() {

int n;
cin>>n;
int original=n;
int armstrongno=0;
while(n>0){
    int l = n%10;
    armstrongno += (int)pow(l,3);
    n=n/10;
}
cout<<armstrongno;
if(original == armstrongno){
    cout<<endl<<"it is armstrong no";

}
else{
    cout << "no";
}

}



