 #include<bits/stdc++.h>

 using namespace std;

 void print(int i,int n){
       if(i<1) return;
       else{
         print(i-1,n);
         cout<<i;
       }
 }


int main(){
    int n;
    cin>>n;
    print(n,n);

}
//print n to 1 nbacktrackinbg;

 #include<bits/stdc++.h>

 using namespace std;

 void print(int i,int n){
       if(i>n) return;
       else{
         print(i+1,n);
         cout<<i;
       }
 }


int main(){
    int n;
    cin>>n;
    print(1,n);

}
