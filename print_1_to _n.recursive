 #include<bits/stdc++.h>

 using namespace std;
 
 void print(int i,int n){
    if(i>n) return ;
    else {
        cout<<i;
        print(i+1,n);  
    }
 }

 int main(){
    int n;
    cin>>n;
    print(1,n);
 }

// print nto 1 recursive 

 #include<bits/stdc++.h>

 using namespace std;
 
 void print(int i,int n){
    if(i<1) return ;
    else {
        cout<<i;
        print(i-1,n);  
    }
 }

 int main(){
    int n;
    cin>>n;
    print(n,n);
 }
